<div class="clear options">
  <%= link_to "&laquo; Back".html_safe, :back %>
</div>

<h2>Delayed Job <%= @job.id %></h2>

<p><%= link_to "Retry", retry_admin_job_path(@job), {method: :post, 'data-confirm' => 'Are you sure?'} %> |
<%= link_to "Delete", admin_job_path(@job), {method: :delete, 'data-confirm' => 'Are you sure?'} %></p>

<table class="details">
  <tr> <th>Attempts</th> <td><%= @job.attempts %></td> </tr>
  <tr> <th>Handler</th> <td><%= @job.handler %></td> </tr>
  <tr> <th>Created At</th> <td><%= @job.created_at %></td> </tr>
  <tr> <th>Run At</th> <td><%= @job.run_at %></td> </tr>
  <tr> <th>Failed At</th> <td><%= @job.failed_at %></td> </tr>
  <tr> <th>Last Error</th> <td><%= @job.last_error %></td> </tr>
</table>
